// https://html.spec.whatwg.org/multipage/grouping-content.html#the-div-element
// https://www.w3.org/TR/html-aria/#el-div
// https://w3c.github.io/html-aria/#el-div
{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": "#flow"
			}
		],
		"conditional": [
			{
				"condition": "dl > div",
				"contents": [
					{
						"zeroOrMore": "#script-supporting"
					},
					{
						"oneOrMore": "dt"
					},
					{
						"zeroOrMore": "#script-supporting"
					},
					{
						"oneOrMore": "dd"
					},
					{
						"zeroOrMore": "#script-supporting"
					}
				]
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
	"implicitRole": {
		"role": false
	},
	"permittedRoles": {
		"roles": true
	}
}
