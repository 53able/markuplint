// https://html.spec.whatwg.org/multipage/grouping-content.html#the-menu-element
// https://www.w3.org/TR/html-aria/#el-menu
{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": ["li", "#script-supporting"]
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
	"implicitRole": {
		"role": "list"
	},
	"permittedRoles": {
		"roles": [
			"directory",
			"group",
			"listbox",
			"menu",
			"menubar",
			"none",
			"presentation",
			"radiogroup",
			"tablist",
			"toolbar",
			"tree"
		]
	}
}
