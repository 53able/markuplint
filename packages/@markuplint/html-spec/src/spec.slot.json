// https://html.spec.whatwg.org/multipage/scripting.html#the-slot-element
// https://www.w3.org/TR/html-aria/#el-slot
// https://w3c.github.io/html-aria/#el-slot
{
	"contentModel": {
		"contents": [
			{
				"transparent": "*"
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {
		// https://html.spec.whatwg.org/multipage/scripting.html#attr-slot-name
		"name": {
			"type": "NoEmptyAny"
		}
	},
	"aria": {
		"implicitRole": false,
		"permittedRoles": false,
		"properties": false
	}
}
