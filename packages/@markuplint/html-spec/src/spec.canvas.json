// https://html.spec.whatwg.org/multipage/canvas.html#the-canvas-element
// https://www.w3.org/TR/html-aria/#el-canvas
{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": "#phrasing",
				"notAllowedDescendants": ["#interactive"]
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true,
		"#HTMLEmbededAndMediaContentAttrs": ["height", "width"]
	},
	"attributes": {
		// https://html.spec.whatwg.org/multipage/canvas.html#attr-canvas-height
		"height": {
			"defaultValue": "150"
		},
		// https://html.spec.whatwg.org/multipage/canvas.html#attr-canvas-width
		"width": {
			"defaultValue": "300"
		}
	},
	"implicitRole": {
		"role": false
	},
	"permittedRoles": {
		"roles": true
	}
}
