// https://html.spec.whatwg.org/multipage/grouping-content.html#the-dt-element
// https://www.w3.org/TR/html-aria/#el-dt
// https://w3c.github.io/html-aria/#el-dt
{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": "#flow",
				"notAllowedDescendants": ["header", "footer", "#sectioning", "#heading"]
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
	"implicitRole": {
		"role": "term"
	},
	"permittedRoles": {
		"roles": ["listitem"]
	}
}
