// https://html.spec.whatwg.org/multipage/grouping-content.html#the-figcaption-element
// https://www.w3.org/TR/html-aria/#el-figcaption
// https://w3c.github.io/html-aria/#el-figcaption
{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": "#flow"
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
	"implicitRole": {
		"role": false
	},
	"permittedRoles": {
		"roles": ["group", "none", "presentation"]
	}
}
