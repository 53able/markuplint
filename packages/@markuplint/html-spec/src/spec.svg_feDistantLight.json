// https://drafts.fxtf.org/filter-effects/#feDistantLightElement
// https://www.w3.org/TR/svg-aam-1.0/#details-id-20
{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": ["#SVGDescriptive", "svg|animate", "svg|script", "svg|set"]
			}
		]
	},
	"globalAttrs": {
		"#SVGCoreAttrs": ["id", "xml:base", "xml:lang", "xml:space"]
	},
	"attributes": {
		// https://drafts.fxtf.org/filter-effects/#element-attrdef-fedistantlight-azimuth
		"azimuth": {
			"type": "<number>",
			"defaultValue": "0",
			"animatable": true
		},
		// https://drafts.fxtf.org/filter-effects/#element-attrdef-fedistantlight-elevation
		"elevation": {
			"type": "<number>",
			"defaultValue": "0",
			"animatable": true
		}
	},
	"aria": {
		"implicitRole": false,
		"permittedRoles": false
	}
}
