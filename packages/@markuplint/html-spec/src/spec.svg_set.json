// https://svgwg.org/specs/animations/#SetElement
// https://www.w3.org/TR/svg-aam-1.0/#details-id-61
{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": [":model(SVGDescriptive)", "svg|script"]
			}
		]
	},
	"globalAttrs": {
		"#GlobalEventAttrs": true,
		"#SVGAnimationEventAttrs": ["onbegin", "onend", "onrepeat"],
		"#SVGAnimationTargetElementAttrs": ["href"],
		"#SVGAnimationAttributeTargetAttrs": ["attributeName"],
		"#SVGAnimationTimingAttrs": [
			"begin",
			"dur",
			"end",
			"min",
			"max",
			"restart",
			"repeatCount",
			"repeatDur",
			"fill"
		],
		"#SVGConditionalProcessingAttrs": ["requiredExtensions", "systemLanguage"],
		"#SVGCoreAttrs": ["id", "tabindex", "autofocus", "lang", "xml:space", "class", "style"]
	},
	"attributes": {
		// https://svgwg.org/specs/animations/#SetElementToAttribute
		"to": {
			// Specifies the value for the attribute during the duration of the \"set\" element. The argument value must match the attribute type. For a list of attributes and properties that can be animated using the \"set\" element, see Elements, attributes and properties that can be animated.
			"type": "NoEmptyAny"
		}
	},
	"aria": {
		"implicitRole": false,
		"permittedRoles": false
	}
}
