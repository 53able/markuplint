// https://html.spec.whatwg.org/multipage/text-level-semantics.html#the-em-element
// https://www.w3.org/TR/html-aria/#el-em
// https://w3c.github.io/html-aria/#el-em
{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": "#phrasing"
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
	"aria": {
		"implicitRole": "emphasis",
		"permittedRoles": true,
		"namingProhibited": true,
		"1.1": {
			"implicitRole": false
		}
	}
}
