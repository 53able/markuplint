// https://html.spec.whatwg.org/multipage/edits.html#the-ins-element
// https://www.w3.org/TR/html-aria/#el-ins
// https://w3c.github.io/html-aria/#el-ins
{
	"contentModel": {
		"contents": [
			{
				"transparent": "*"
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {
		"cite": {
			// https://html.spec.whatwg.org/multipage/edits.html#attr-mod-cite
			"type": "URL"
		},
		"datetime": {
			// https://html.spec.whatwg.org/multipage/edits.html#attr-mod-datetime
			"type": "DateTime"
		}
	},
	"aria": {
		"implicitRole": "insertion",
		"permittedRoles": true,
		"namingProhibited": true,
		"1.1": {
			"implicitRole": false
		}
	}
}
