// https://html.spec.whatwg.org/multipage/semantics.html#the-html-element
// https://www.w3.org/TR/html-aria/#el-html
{
	"contentModel": {
		"contents": [
			{
				"require": "head"
			},
			{
				"require": "body"
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
	"implicitRole": {
		"role": "document"
	},
	"permittedRoles": {
		"roles": false,
		"properties": false
	}
}
