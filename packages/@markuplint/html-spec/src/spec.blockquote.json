// https://html.spec.whatwg.org/multipage/grouping-content.html#the-blockquote-element
// https://www.w3.org/TR/html-aria/#el-blockquote
// https://w3c.github.io/html-aria/#el-blockquote
{
	"contentModel": {
		"contents": [
			{
				"oneOrMore": ":model(flow)"
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {
		// https://html.spec.whatwg.org/multipage/grouping-content.html#attr-blockquote-cite
		"cite": {
			"type": "URL"
		}
	},
	"aria": {
		"implicitRole": "blockquote",
		"permittedRoles": true,
		"1.1": {
			"implicitRole": false
		}
	}
}
