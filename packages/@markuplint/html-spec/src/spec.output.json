// https://html.spec.whatwg.org/multipage/form-elements.html#the-output-element
// https://www.w3.org/TR/html-aria/#el-output
// https://w3c.github.io/html-aria/#el-output
{
	"contentModel": {
		"contents": [
			{
				"oneOrMore": ":model(phrasing)"
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true,
		"#HTMLFormControlElementAttrs": ["form", "name"]
	},
	"attributes": {
		// https://html.spec.whatwg.org/multipage/form-elements.html#attr-output-for
		"for": {
			"type": "DOMID"
		}
	},
	"aria": {
		"implicitRole": "status",
		"permittedRoles": false
	}
}
