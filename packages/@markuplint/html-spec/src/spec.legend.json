{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": ["#phrasing", "#heading"]
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
	"implicitRole": {
		"role": false
	},
	"permittedRoles": {
		"roles": false,
		"properties": {
			"global": true
		}
	}
}
