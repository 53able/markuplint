// https://html.spec.whatwg.org/multipage/form-elements.html#the-legend-element
// https://www.w3.org/TR/html-aria/#el-legend
// https://w3c.github.io/html-aria/#el-legend
{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": [":model(phrasing)", ":model(heading)"]
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
	"aria": {
		"implicitRole": false,
		"permittedRoles": false,
		"namingProhibited": true
	}
}
