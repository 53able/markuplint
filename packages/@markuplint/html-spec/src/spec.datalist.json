// https://html.spec.whatwg.org/multipage/form-elements.html#the-datalist-element
// https://www.w3.org/TR/html-aria/#el-datalist
{
	"contentModel": {
		"contents": [
			{
				"choice": [
					[
						{
							"zeroOrMore": "#phrasing"
						}
					],
					[
						{
							"zeroOrMore": ["option", "#script-supporting"]
						}
					]
				]
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
	"implicitRole": {
		"role": "listbox"
	},
	"permittedRoles": {
		"roles": false,
		"properties": {
			"global": true,
			"role": "listbox"
		}
	}
}
