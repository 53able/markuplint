// https://html.spec.whatwg.org/multipage/sections.html#the-nav-element
// https://www.w3.org/TR/html-aria/#el-nav
// https://w3c.github.io/html-aria/#el-nav
{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": "#flow"
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
	"implicitRole": {
		"role": "navigation"
	},
	"permittedRoles": {
		"roles": ["group", "menu", "menubar", "none", "presentation", "tablist"]
	}
}
