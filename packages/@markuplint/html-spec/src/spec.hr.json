// https://html.spec.whatwg.org/multipage/grouping-content.html#the-hr-element
// https://www.w3.org/TR/html-aria/#el-hr
// https://w3c.github.io/html-aria/#el-hr
{
	"contentModel": {
		"contents": false
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
	"aria": {
		"implicitRole": "separator",
		"permittedRoles": ["presentation", "none"],
		"properties": {
			"global": true,
			"role": "separator"
		}
	}
}
