{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": "#flow",
				"notAllowedDescendants": ["address", "#heading", "#sectioning", "header", "footer"]
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
