// https://html.spec.whatwg.org/multipage/tables.html#the-tr-element
// https://www.w3.org/TR/html-aria/#el-tr
// https://w3c.github.io/html-aria/#el-tr
{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": ["th", "td", "#script-supporting"]
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
	"implicitRole": {
		"role": "row"
	},
	"permittedRoles": {
		// No role if the ancestor table element has role=table, grid, or treegrid; otherwise any role.
		"roles": true,
		"conditions": [
			{
				"condition": "table:is(:not([role]), [role=table], [role=grid], [role=treegrid]) > :is(thead, tfoot, tbody) > tr",
				"roles": false
			}
		]
	}
}
