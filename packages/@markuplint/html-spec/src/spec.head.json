{
	"contentModel": {
		"contents": [
			{
				"zeroOrMore": ":model(metadata):not(title)"
			},
			{
				"require": "title"
			},
			{
				"zeroOrMore": ":model(metadata):not(title)"
			}
		]
	},
	"globalAttrs": {
