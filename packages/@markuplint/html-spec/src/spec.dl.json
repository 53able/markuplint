{
	"contentModel": {
		"contents": [
			{
				"choice": [
					[
						{
							"oneOrMore": [
								{
									"zeroOrMore": "#script-supporting"
								},
								{
									"oneOrMore": "dt"
								},
								{
									"zeroOrMore": "#script-supporting"
								},
								{
									"oneOrMore": "dd"
								},
								{
									"zeroOrMore": "#script-supporting"
								}
							]
						}
					],
					[
						{
							"oneOrMore": ["div", "#script-supporting"]
						}
					]
				]
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true
	},
	"attributes": {},
	"implicitRole": {
		"role": false
	},
	"permittedRoles": {
		"roles": ["group", "list", "none", "presentation"]
	}
