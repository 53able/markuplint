// https://html.spec.whatwg.org/multipage/form-elements.html#the-fieldset-element
// https://www.w3.org/TR/html-aria/#el-fieldset
// https://w3c.github.io/html-aria/#el-fieldset
{
	"contentModel": {
		"contents": [
			{
				"optional": "legend"
			},
			{
				"oneOrMore": ":model(flow)"
			}
		]
	},
	"globalAttrs": {
		"#HTMLGlobalAttrs": true,
		"#GlobalEventAttrs": true,
		"#DocumentElementEventAttrs": true,
		"#ARIAAttrs": true,
		"#HTMLFormControlElementAttrs": ["disabled", "form", "name"]
	},
	"attributes": {},
	"aria": {
		"implicitRole": "group",
		"permittedRoles": ["radiogroup", "presentation", "none"]
	}
}
