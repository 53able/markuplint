{
	"tag": "meshgradient",
	"attributes": [
		"#SVGCoreAttrs",
		"#SVGStylingAttrs",
		"#SVGPresentationAttrs",
		"#SVGGlobalEventAttrs",
		{
			"name": "x",
			"type": ["SVGLength", "SVGPercentage"],
			"defaultValue": "0%"
		},
		{
			"name": "y",
			"type": ["SVGLength", "SVGPercentage"],
			"defaultValue": "0%"
		},
		{
			"name": "gradientUnits",
			"type": {
				"enum": ["userSpaceOnUse", "objectBoundingBox"]
			},
			"defaultValue": "objectBoundingBox"
		},
		{
			"name": "transform",
			"type": "CSSTransformList"
		},
		{
			"name": "href",
			"type": "URL"
		},
		{
			"name": "type",
			"type": {
				"enum": ["bilinear", "bicubic"]
			},
			"defaultValue": "bilinear"
		}
	]
}
