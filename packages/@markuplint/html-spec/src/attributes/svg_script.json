{
	"tag": "script",
	"attributes": [
		"#SVGCoreAttrs",
		"#SVGStylingAttrs",
		"#SVGGlobalEventAttrs",
		"#SVGDocumentElementEventAttrs",
		{
			"name": "crossorigin",
			"type": "Crossorigin"
		},
		{
			"name": "href",
			"type": "URL"
		},
		{
			"name": "type",
			"type": "MIMEType",
			"defaultValue": "application/ecmascript"
		},
		{
			"name": "xlink:href",
			"type": "URL"
		}
	]
}
