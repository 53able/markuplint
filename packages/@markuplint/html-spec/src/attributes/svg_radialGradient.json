		},
		{
			"name": "cy",
			"type": ["SVGLength", "SVGPercentage"],
			"defaultValue": "50%"
		},
		{
			"name": "r",
			"type": ["SVGLength", "SVGPercentage"],
			"defaultValue": "50%"
		},
		{
			"name": "fx",
			"type": ["SVGLength", "SVGPercentage"]
		},
		{
			"name": "fy",
			"type": ["SVGLength", "SVGPercentage"]
		},
		{
			"name": "fr",
			"type": "SVGLength",
			"defaultValue": "0"
		},
		{
