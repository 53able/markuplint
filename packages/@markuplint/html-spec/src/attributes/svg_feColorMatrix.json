			"defaultValue": "matrix"
		},
		{
			"name": "values",
			"type": "SVGColorMatrix"
		},
		{
			"name": "height",
			"type": ["SVGLength", "SVGPercentage"],
			"defaultValue": "100%"
		},
		{
			"name": "width",
			"type": ["SVGLength", "SVGPercentage"],
			"defaultValue": "100%"
		}
	]
}
