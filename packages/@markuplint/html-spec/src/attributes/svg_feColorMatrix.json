		"#SVGPresentationAttrs",
		"#SVGFilterPrimitiveAttrs",
		{
			"name": "in",
			"type": [
				{
					"enum": [
						"SourceGraphic",
						"SourceAlpha",
						"BackgroundImage",
						"BackgroundAlpha",
						"FillPaint",
						"StrokePaint"
					]
				},
				"CSSCustomIdent"
			]
		},
		{
			"name": "type",
			"type": {
				"enum": ["matrix", "saturate", "hueRotate", "luminanceToAlpha"]
