{
	"tag": "linearGradient",
	"attributes": [
		"#SVGCoreAttrs",
		"#SVGStylingAttrs",
		"#SVGGlobalEventAttrs",
		"#SVGDocumentElementEventAttrs",
		"#SVGPresentationAttrs",
		"#XLinkAttrs",
		{
			"name": "gradientUnits",
			"type": {
				"enum": ["userSpaceOnUse", "objectBoundingBox"]
			},
			"defaultValue": "objectBoundingBox"
		},
		{
			"name": "gradientTransform",
			"type": "CSSTransformList"
		},
		{
			"name": "href",
			"type": "URL"
		},
		{
			"name": "spreadMethod",
			"type": {
				"enum": ["pad", "reflect", "repeat"]
			},
			"defaultValue": "pad"
		},
		{
			"name": "x1",
			"type": ["SVGLength", "SVGPercentage"],
			"defaultValue": "0%"
		},
		{
			"name": "x2",
			"type": ["SVGLength", "SVGPercentage"],
			"defaultValue": "100%"
		},
		{
			"name": "y1",
			"type": ["SVGLength", "SVGPercentage"],
			"defaultValue": "0%"
		},
		{
			"name": "y2",
			"type": ["SVGLength", "SVGPercentage"],
			"defaultValue": "0%"
		}
	]
}
