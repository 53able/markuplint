{
  "root": true,
  "extends": ["../.eslintrc", "plugin:react/recommended", "plugin:react-hooks/recommended"],
  "plugins": ["react"],
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "react/display-name": 0,
    "react/prop-types": 0
  },
  "globals": {
    "React": true
  }
}
